{
  "name": "xpressengine-plugin/ckeditor",
  "description": "XpressEngine's plugin for bundle (XE)",
  "keywords": ["xpressengine bundle", "XE CkEditor"],
  "version": "0.4.0",
  "support": {
    "email": "developers@xpressengine.com",
    "issues": "http://xpressengine.com/issues",
    "forum": "http://xpressengine.com/forum",
    "wiki": "http://xpressengine.com/wiki",
    "irc": "http://xpressengine.com/irc",
    "source": "http://xpressengine.com/source/"
  },
  "authors": [
    {
      "name": "akasima",
      "email": "akasima@xpressengine.com",
      "homepage": "http://www.xpressengine.com",
      "role": "Developer"
    }
  ],
  "license": "LGPL",
  "type": "xpressengine-plugin",
  "extra": {
    "xpressengine": {
      "title": "CkEditor",
      "component": {
        "uiobject/xpressengine@ckEditor": {
          "class": "Xpressengine\\Plugins\\CkEditor\\UIObject\\CkEditor",
          "name": "CkEditor",
          "description": "XE 에서 사용할 기본 에디터"
        },
        "uiobject/xpressengine@contentsCompiler": {
          "class": "Xpressengine\\Plugins\\CkEditor\\UIObject\\ContentsCompiler",
          "name": "ContentsCompiler",
          "description": "에디터로 작성된 내용 출력시 사용"
        },
        "FieldType/xpressengine@CkEditor": {
          "class": "Xpressengine\\Plugins\\CkEditor\\FieldType\\CkEditor",
          "name": "CkEditor",
          "description": "에디터 Dynamic field"
        }
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "Xpressengine\\Plugins\\CkEditor\\": "src/"
    }
  }
}
